{
    "compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "strict": true,
        "noEmit": true,
        "skipLibCheck": true,
        "baseUrl": ".",
        "jsx": "react-jsx",
        "paths": {
            "@/*": [
                "./*"
            ],
            "@shared/*": [
                "./*"
            ]
        }
    },
    "include": [
        "**/*"
    ]
}